{
  "name": "mb/angelshop-webpage",
  "description": "description_text",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Michael Blunck",
      "email": "mi.blunck@gmail.com"
    }
  ],
  "extra": {
    "typo3/cms": {
      "composer-mode": true,
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
